        <Param val="" valType="extendedCode" updates="constant" name="End JS Routine"/>
        <Param val="#Convert corrDig, which contains integers, to a list of strings&amp;#10;corrDig_str = [str(digit) for digit in corrDig]  # Convert each integer in corrDig to a string&amp;#10;&amp;#10;#To display the correct digits to the participants&amp;#10;corrDig_disp = ' '.join(map(str, corrDig))  # Convert each number to a string and join with spaces&amp;#10;&amp;#10;if clicked_digits == corrDig_str:&amp;#10;    feedback = &quot;Correct!&quot;&amp;#10;    resp_prac = 1&amp;#10;else:&amp;#10;    feedback = &quot;Incorrect&quot; #&quot;Incorrect. The correct sequence was &quot; + corrDig_disp&amp;#10;    resp_prac = 0&amp;#10;&amp;#10;thisExp.addData('score', resp_prac)  # Recond wheter correct or incorrect&amp;#10;print(f'Clicked digits: {clicked_digits}')&amp;#10;print(f'Correct sequence: {corrDig_str}')" valType="extendedCode" updates="constant" name="End Routine"/>
