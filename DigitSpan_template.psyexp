      <CodeComponent name="code_off" plugin="None">
        <Param val="from psychopy import core" valType="extendedCode" updates="constant" name="Before Experiment"/>
        <Param val="" valType="extendedCode" updates="constant" name="Before JS Experiment"/>
        <Param val="count_incorrect = 0  # To count consecutive incorrect responses&amp;#10;max_incorrect = 2    # Set the threshold for consecutive incorrect answers&amp;#10;" valType="extendedCode" updates="constant" name="Begin Experiment"/>
        <Param val="count_incorrect = 0;&amp;#10;max_incorrect = 2;&amp;#10;" valType="extendedCode" updates="constant" name="Begin JS Experiment"/>
        <Param val="let clicked_digits = [];&amp;#10;let isPressed = false;&amp;#10;&amp;#10;// Initialize the digits (make sure these are defined correctly)&amp;#10;let digits = [off_digit_1, off_digit_2, off_digit_3, off_digit_4, off_digit_5, off_digit_6, off_digit_7, off_digit_8, off_digit_9];&amp;#10;&amp;#10;// Dictionary to hold each digit's original size&amp;#10;let original_sizes = {};&amp;#10;digits.forEach(num =&gt; {&amp;#10;    original_sizes[num] = num.height; // Store original sizes&amp;#10;});&amp;#10;&amp;#10;// Initialize click_clocks&amp;#10;let click_clocks = {};&amp;#10;digits.forEach(num =&gt; {&amp;#10;    click_clocks[num] = new util.Clock(); // Correctly initialize clocks&amp;#10;});" valType="extendedCode" updates="constant" name="Begin JS Routine"/>
        <Param val="clicked_digits = []&amp;#10;is_pressed = False&amp;#10;&amp;#10;# Dictionary to hold each digit's original size&amp;#10;original_sizes = {num: num.height for num in [off_digit_1, off_digit_2, off_digit_3, off_digit_4, off_digit_5, off_digit_6, off_digit_7, off_digit_8, off_digit_9]}&amp;#10;# Dictionary to track the clock for each digit&amp;#10;click_clocks = {num: core.Clock() for num in [off_digit_1, off_digit_2, off_digit_3, off_digit_4, off_digit_5, off_digit_6, off_digit_7, off_digit_8, off_digit_9]}" valType="extendedCode" updates="constant" name="Begin Routine"/>
        <Param val="Both" valType="str" updates="None" name="Code Type"/>
        <Param val="# Set the duration for which the digit remains enlarged (in seconds)&amp;#10;enlarge_duration = 0.2&amp;#10;&amp;#10;if mouse_off.getPressed()[0]:  # Left mouse button is currently pressed&amp;#10;    if not is_pressed:  # Only proceed if this is a new click (debouncing logic)&amp;#10;        # Check if the &quot;proceed&quot; text was clicked&amp;#10;        if mouse_off.isPressedIn(off_proceed):&amp;#10;            continueRoutine = False  # End the routine&amp;#10;        else:  # Check if a number is clicked&amp;#10;            for num in [off_digit_1, off_digit_2, off_digit_3, off_digit_4, off_digit_5, off_digit_6, off_digit_7, off_digit_8, off_digit_9]:&amp;#10;                if mouse_off.isPressedIn(num):&amp;#10;                    # Enlarge the digit and reset its clock for the enlarge duration&amp;#10;                    num.height = original_sizes[num] * 1.5  # Increase size by 50%&amp;#10;                    click_clocks[num].reset()  # Reset clock to track enlarge duration&amp;#10;                    clicked_digits.append(num.text)  # Store the text of the clicked digit&amp;#10;                    &amp;#10;                    # Store the clicked digit text in the experiment data immediately after clicking&amp;#10;                    thisExp.addData('clicked_digit', num.text)&amp;#10;                    print(f&quot;Clicked digit: {num.text}&quot;)  # Print clicked digit for debugging&amp;#10;                    &amp;#10;        # Set is_pressed to True after a click is registered&amp;#10;        is_pressed = True&amp;#10;&amp;#10;else:&amp;#10;    # Reset is_pressed to False when the mouse button is released, enabling new clicks&amp;#10;    is_pressed = False&amp;#10;&amp;#10;# Revert size back to original if the enlarge duration has passed&amp;#10;for num, clock in click_clocks.items():&amp;#10;    if clock.getTime() &gt; enlarge_duration:  # Check if the enlarge time has passed&amp;#10;        num.height = original_sizes[num]  # Reset to original size&amp;#10;&amp;#10;# At the end of the routine, add the entire list of clicked digits for analysis&amp;#10;thisExp.addData('all_clicked_digits', ', '.join(clicked_digits))  # Store all clicked digits&amp;#10;print(f&quot;All clicked digits: {clicked_digits}&quot;)  # Print all clicked digits for debugging" valType="extendedCode" updates="constant" name="Each Frame"/>
